# EC-module test

[testA]
TestPathName	     = CFtests/testA/
useBcBlockApproach   = T                                     # approach according to CF_DLL
QuantityName         = water_level                           # name of quantity 
LocationName         = bound_1                               # name of the bound./lat. location
LocationIsLateral    = F                                     # is the location a lateral?
InputFileName        = BoundaryConditions.bc                 # name of input 
InputFileType        = bc                                    # type of input
RefFileName          = testA.ref                             # Reference file for comparing output
Tolerance            = 0.0000001                             # test tolerance
Times                = 57727.00833333, 57727.4375, 57727.75347222, 57728.0, 57728.07638889 # times for requesting data from EC-module

